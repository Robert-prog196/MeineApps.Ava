<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:HandwerkerRechner.ViewModels.Premium"
             xmlns:mi="using:Material.Icons.Avalonia"
             xmlns:conv="using:HandwerkerRechner.Converters"
             xmlns:loc="using:MeineApps.Core.Ava.Localization"
             xmlns:skia="using:Avalonia.Labs.Controls"
             x:Class="HandwerkerRechner.Views.Premium.RoofSolarView"
             x:DataType="vm:RoofSolarViewModel">

  <UserControl.Resources>
    <conv:IntEqualsConverter x:Key="IntEquals" />
  </UserControl.Resources>

  <Grid RowDefinitions="Auto,*,Auto"
        AutomationProperties.AutomationId="RoofSolar_RootGrid">

    <!-- Header -->
    <Border Grid.Row="0"
            Background="{DynamicResource AccentBrush}"
            Padding="16,12,16,20"
            CornerRadius="0,0,24,24"
            AutomationProperties.AutomationId="RoofSolar_HeaderBorder">
      <Grid ColumnDefinitions="Auto,*">
        <Button Command="{Binding GoBackCommand}"
                Background="#33FFFFFF"
                CornerRadius="20"
                Width="40" Height="40"
                Padding="0"
                AutomationProperties.AutomationId="RoofSolar_BtnBack">
          <mi:MaterialIcon Kind="ArrowLeft" Width="22" Height="22" Foreground="White" />
        </Button>
        <StackPanel Grid.Column="1" Orientation="Horizontal" Spacing="12"
                    Margin="12,0,0,0" VerticalAlignment="Center">
          <Border Background="#33FFFFFF" CornerRadius="12" Width="40" Height="40">
            <mi:MaterialIcon Kind="SolarPanel" Width="22" Height="22"
                             Foreground="White"
                             HorizontalAlignment="Center" VerticalAlignment="Center" />
          </Border>
          <TextBlock Text="{loc:Translate RoofSolarCalculator}"
                     FontSize="20" FontWeight="Bold" Foreground="White"
                     VerticalAlignment="Center"
                     AutomationProperties.AutomationId="RoofSolar_TxtTitle" />
        </StackPanel>
      </Grid>
    </Border>

    <!-- Content -->
    <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto"
                  AutomationProperties.AutomationId="RoofSolar_ScrollViewer">
      <StackPanel Spacing="16" Margin="16,16,16,200">

        <!-- Calculator Selector -->
        <Border Classes="Card StatsCard"
                AutomationProperties.AutomationId="RoofSolar_CardSelector">
          <ComboBox ItemsSource="{Binding Calculators}"
                    SelectedIndex="{Binding SelectedCalculator}"
                    HorizontalAlignment="Stretch"
                    AutomationProperties.AutomationId="RoofSolar_CmbCalculator" />
        </Border>

        <!-- Visualisierung -->
        <Border Classes="Card" Padding="0" Height="220" Margin="0,0,0,0"
                IsVisible="{Binding HasResult}" ClipToBounds="True" CornerRadius="12"
                AutomationProperties.AutomationId="RoofSolar_CardVisualization">
          <skia:SKCanvasView x:Name="RoofSolarCanvas" PaintSurface="OnPaintVisualization"
                             AutomationProperties.AutomationId="RoofSolar_Canvas" />
        </Border>

        <!-- Calculator 0: Roof Pitch -->
        <Panel IsVisible="{Binding SelectedCalculator, Converter={StaticResource IntEquals}, ConverterParameter=0}">
          <StackPanel Spacing="16">

            <!-- Input -->
            <Border Classes="Card StatsCard"
                    AutomationProperties.AutomationId="RoofSolar_CardPitchInput">
              <StackPanel Spacing="12">
                <TextBlock Text="{loc:Translate RoofPitch}"
                           FontSize="16" FontWeight="SemiBold"
                           Foreground="{DynamicResource TextPrimaryBrush}" />

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate Run}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding Run}" Watermark="m"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxRun" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate Rise}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding Rise}" Watermark="m"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxRise" />
                </StackPanel>
              </StackPanel>
            </Border>

            <!-- Results -->
            <Border Classes="Card Elevated StatsCard"
                    IsVisible="{Binding HasResult}"
                    AutomationProperties.AutomationId="RoofSolar_CardPitchResult">
              <StackPanel Spacing="10">
                <TextBlock Text="{loc:Translate Result}"
                           FontSize="16" FontWeight="SemiBold"
                           Foreground="{DynamicResource PrimaryBrush}"
                           AutomationProperties.AutomationId="RoofSolar_TxtPitchResultHeader" />

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate UnitDegrees}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding PitchResult.PitchDegrees}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtPitchDegrees" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate UnitPercent}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding PitchResult.PitchPercent}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtPitchPercent" />
                </Grid>

                <!-- Aktions-Buttons: Share + PDF Export -->
                <Grid ColumnDefinitions="*,8,*" Margin="0,4,0,0">
                  <Button Grid.Column="0"
                          Command="{Binding ShareResultCommand}"
                          Classes="Outlined"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Center"
                          AutomationProperties.AutomationId="RoofSolar_BtnSharePitch">
                    <StackPanel Orientation="Horizontal" Spacing="4">
                      <mi:MaterialIcon Kind="ContentCopy" Width="16" Height="16"
                                       Foreground="{DynamicResource AccentBrush}" />
                      <TextBlock Text="{loc:Translate ShareResult}"
                                 FontSize="12" TextTrimming="CharacterEllipsis"
                                 VerticalAlignment="Center" />
                    </StackPanel>
                  </Button>
                  <Button Grid.Column="2"
                          Command="{Binding ExportMaterialListCommand}"
                          Classes="Outlined"
                          IsEnabled="{Binding !IsExporting}"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Center"
                          AutomationProperties.AutomationId="RoofSolar_BtnExportPitch">
                    <StackPanel Orientation="Horizontal" Spacing="4">
                      <mi:MaterialIcon Kind="FilePdfBox" Width="16" Height="16"
                                       Foreground="{DynamicResource AccentBrush}" />
                      <TextBlock Text="{loc:Translate ExportMaterialList}"
                                 FontSize="12" TextTrimming="CharacterEllipsis"
                                 VerticalAlignment="Center" />
                    </StackPanel>
                  </Button>
                </Grid>
              </StackPanel>
            </Border>

          </StackPanel>
        </Panel>

        <!-- Calculator 1: Roof Tiles -->
        <Panel IsVisible="{Binding SelectedCalculator, Converter={StaticResource IntEquals}, ConverterParameter=1}">
          <StackPanel Spacing="16">

            <!-- Input -->
            <Border Classes="Card StatsCard"
                    AutomationProperties.AutomationId="RoofSolar_CardTilesInput">
              <StackPanel Spacing="12">
                <TextBlock Text="{loc:Translate RoofTiles}"
                           FontSize="16" FontWeight="SemiBold"
                           Foreground="{DynamicResource TextPrimaryBrush}" />

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate UnitSquareMeters}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding RoofArea}" Watermark="m²"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxRoofArea" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate TilesPerSqm}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding TilesPerSqm}" Watermark="pcs/m²"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxTilesPerSqm" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate CostCalculation}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding PricePerTile}"
                           Watermark="EUR"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxPricePerTile" />
                </StackPanel>
              </StackPanel>
            </Border>

            <!-- Results -->
            <Border Classes="Card Elevated StatsCard"
                    IsVisible="{Binding HasResult}"
                    AutomationProperties.AutomationId="RoofSolar_CardTilesResult">
              <StackPanel Spacing="10">
                <TextBlock Text="{loc:Translate Result}"
                           FontSize="16" FontWeight="SemiBold"
                           Foreground="{DynamicResource PrimaryBrush}"
                           AutomationProperties.AutomationId="RoofSolar_TxtTilesResultHeader" />

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate UnitPieces}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding TilesResult.TilesNeeded}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtTilesNeeded" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate TilesWithReserve}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding TilesResult.TilesWithReserve}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtTilesWithReserve" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto"
                      IsVisible="{Binding ShowTileCost}">
                  <TextBlock Grid.Column="0" Text="{loc:Translate TotalCost}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding RoofTileCostDisplay}"
                             FontSize="17" FontWeight="Bold"
                             Foreground="{DynamicResource AccentBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtTileCost" />
                </Grid>

                <!-- Aktions-Buttons: Share + PDF Export -->
                <Grid ColumnDefinitions="*,8,*" Margin="0,4,0,0">
                  <Button Grid.Column="0"
                          Command="{Binding ShareResultCommand}"
                          Classes="Outlined"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Center"
                          AutomationProperties.AutomationId="RoofSolar_BtnShareTiles">
                    <StackPanel Orientation="Horizontal" Spacing="4">
                      <mi:MaterialIcon Kind="ContentCopy" Width="16" Height="16"
                                       Foreground="{DynamicResource AccentBrush}" />
                      <TextBlock Text="{loc:Translate ShareResult}"
                                 FontSize="12" TextTrimming="CharacterEllipsis"
                                 VerticalAlignment="Center" />
                    </StackPanel>
                  </Button>
                  <Button Grid.Column="2"
                          Command="{Binding ExportMaterialListCommand}"
                          Classes="Outlined"
                          IsEnabled="{Binding !IsExporting}"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Center"
                          AutomationProperties.AutomationId="RoofSolar_BtnExportTiles">
                    <StackPanel Orientation="Horizontal" Spacing="4">
                      <mi:MaterialIcon Kind="FilePdfBox" Width="16" Height="16"
                                       Foreground="{DynamicResource AccentBrush}" />
                      <TextBlock Text="{loc:Translate ExportMaterialList}"
                                 FontSize="12" TextTrimming="CharacterEllipsis"
                                 VerticalAlignment="Center" />
                    </StackPanel>
                  </Button>
                </Grid>
              </StackPanel>
            </Border>

          </StackPanel>
        </Panel>

        <!-- Calculator 2: Solar Yield -->
        <Panel IsVisible="{Binding SelectedCalculator, Converter={StaticResource IntEquals}, ConverterParameter=2}">
          <StackPanel Spacing="16">

            <!-- Input -->
            <Border Classes="Card StatsCard"
                    AutomationProperties.AutomationId="RoofSolar_CardSolarInput">
              <StackPanel Spacing="12">
                <TextBlock Text="{loc:Translate SolarYield}"
                           FontSize="16" FontWeight="SemiBold"
                           Foreground="{DynamicResource TextPrimaryBrush}" />

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate UnitSquareMeters}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding SolarRoofArea}" Watermark="m²"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxSolarRoofArea" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate PanelEfficiency}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding PanelEfficiency}" Watermark="%"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxPanelEfficiency" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate Orientation}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <ComboBox ItemsSource="{Binding Orientations}"
                            SelectedIndex="{Binding SelectedOrientation}"
                            HorizontalAlignment="Stretch"
                            AutomationProperties.AutomationId="RoofSolar_CmbOrientation" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate TiltDegrees}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding TiltDegrees}" Watermark="°"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxTiltDegrees" />
                </StackPanel>

                <StackPanel Spacing="4">
                  <TextBlock Text="{loc:Translate CostCalculation}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBox Text="{Binding SolarSystemCost}"
                           Watermark="EUR"
                           AutomationProperties.AutomationId="RoofSolar_TxtBoxSolarSystemCost" />
                </StackPanel>
              </StackPanel>
            </Border>

            <!-- Results -->
            <Border Classes="Card Elevated StatsCard"
                    IsVisible="{Binding HasResult}"
                    AutomationProperties.AutomationId="RoofSolar_CardSolarResult">
              <StackPanel Spacing="10">
                <TextBlock Text="{loc:Translate Result}"
                           FontSize="16" FontWeight="SemiBold"
                           Foreground="{DynamicResource PrimaryBrush}"
                           AutomationProperties.AutomationId="RoofSolar_TxtSolarResultHeader" />

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate UsableArea}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding SolarResult.UsableArea}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtUsableArea" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate KwPeak}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding SolarResult.KwPeak}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtKwPeak" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto">
                  <TextBlock Grid.Column="0" Text="{loc:Translate AnnualYield}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding SolarResult.AnnualYieldKwh}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtAnnualYield" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto"
                      IsVisible="{Binding ShowSolarCost}">
                  <TextBlock Grid.Column="0" Text="{loc:Translate TotalCost}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding SolarCostDisplay}"
                             FontSize="17" FontWeight="Bold"
                             Foreground="{DynamicResource AccentBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtSolarCost" />
                </Grid>

                <Grid ColumnDefinitions="*,Auto"
                      IsVisible="{Binding ShowSolarCost}">
                  <TextBlock Grid.Column="0" Text="{loc:Translate PaybackTime}"
                             FontSize="13"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
                  <TextBlock Grid.Column="1" Text="{Binding PaybackTimeDisplay}"
                             FontSize="15" FontWeight="SemiBold"
                             Foreground="{DynamicResource TextPrimaryBrush}"
                             AutomationProperties.AutomationId="RoofSolar_TxtPaybackTime" />
                </Grid>

                <!-- Aktions-Buttons: Share + PDF Export -->
                <Grid ColumnDefinitions="*,8,*" Margin="0,4,0,0">
                  <Button Grid.Column="0"
                          Command="{Binding ShareResultCommand}"
                          Classes="Outlined"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Center"
                          AutomationProperties.AutomationId="RoofSolar_BtnShareSolar">
                    <StackPanel Orientation="Horizontal" Spacing="4">
                      <mi:MaterialIcon Kind="ContentCopy" Width="16" Height="16"
                                       Foreground="{DynamicResource AccentBrush}" />
                      <TextBlock Text="{loc:Translate ShareResult}"
                                 FontSize="12" TextTrimming="CharacterEllipsis"
                                 VerticalAlignment="Center" />
                    </StackPanel>
                  </Button>
                  <Button Grid.Column="2"
                          Command="{Binding ExportMaterialListCommand}"
                          Classes="Outlined"
                          IsEnabled="{Binding !IsExporting}"
                          HorizontalAlignment="Stretch"
                          HorizontalContentAlignment="Center"
                          AutomationProperties.AutomationId="RoofSolar_BtnExportSolar">
                    <StackPanel Orientation="Horizontal" Spacing="4">
                      <mi:MaterialIcon Kind="FilePdfBox" Width="16" Height="16"
                                       Foreground="{DynamicResource AccentBrush}" />
                      <TextBlock Text="{loc:Translate ExportMaterialList}"
                                 FontSize="12" TextTrimming="CharacterEllipsis"
                                 VerticalAlignment="Center" />
                    </StackPanel>
                  </Button>
                </Grid>
              </StackPanel>
            </Border>

          </StackPanel>
        </Panel>

      </StackPanel>
    </ScrollViewer>

    <!-- Buttons -->
    <Border Grid.Row="2" Padding="16,12" Background="{DynamicResource BackgroundBrush}"
            AutomationProperties.AutomationId="RoofSolar_BottomBar">
      <Grid ColumnDefinitions="*,8,*,8,*">
        <Button Grid.Column="0"
                Command="{Binding ResetCommand}"
                Classes="Outlined"
                HorizontalAlignment="Stretch"
                HorizontalContentAlignment="Center"
                AutomationProperties.AutomationId="RoofSolar_BtnReset">
          <StackPanel Orientation="Horizontal" Spacing="4">
            <mi:MaterialIcon Kind="Refresh" Width="16" Height="16"
                             Foreground="{DynamicResource TextSecondaryBrush}" />
            <TextBlock Text="{loc:Translate Reset}"
                       FontSize="12" TextTrimming="CharacterEllipsis"
                       VerticalAlignment="Center" />
          </StackPanel>
        </Button>
        <Button Grid.Column="2"
                Command="{Binding CalculateCommand}"
                Classes="Primary"
                HorizontalAlignment="Stretch"
                HorizontalContentAlignment="Center"
                AutomationProperties.AutomationId="RoofSolar_BtnCalculate">
          <StackPanel Orientation="Horizontal" Spacing="4">
            <mi:MaterialIcon Kind="Calculator" Width="16" Height="16" />
            <TextBlock Text="{loc:Translate Calculate}"
                       FontSize="12" TextTrimming="CharacterEllipsis"
                       VerticalAlignment="Center" />
          </StackPanel>
        </Button>
        <!-- Save Project -->
        <Button Grid.Column="4"
                Classes="Secondary"
                Command="{Binding SaveProjectCommand}"
                IsVisible="{Binding HasResult}"
                HorizontalAlignment="Stretch"
                HorizontalContentAlignment="Center"
                AutomationProperties.AutomationId="RoofSolar_BtnSave">
          <StackPanel Orientation="Horizontal" Spacing="4">
            <mi:MaterialIcon Kind="ContentSave" Width="16" Height="16" />
            <TextBlock Text="{loc:Translate Save}"
                       FontSize="12" TextTrimming="CharacterEllipsis"
                       VerticalAlignment="Center" />
          </StackPanel>
        </Button>
      </Grid>
    </Border>

    <!-- Save Project Dialog Overlay -->
    <Border Grid.Row="0" Grid.RowSpan="3"
            ZIndex="100"
            Background="#80000000"
            IsVisible="{Binding ShowSaveDialog}"
            AutomationProperties.AutomationId="RoofSolar_SaveDialogOverlay">
      <Border Background="{DynamicResource SurfaceBrush}"
              CornerRadius="16"
              Padding="24"
              MaxWidth="340"
              HorizontalAlignment="Center"
              VerticalAlignment="Center"
              AutomationProperties.AutomationId="RoofSolar_SaveDialogCard">
        <StackPanel Spacing="16">
          <TextBlock Text="{loc:Translate SaveProject}"
                     FontSize="18" FontWeight="Bold"
                     Foreground="{DynamicResource TextPrimaryBrush}"
                     HorizontalAlignment="Center"
                     AutomationProperties.AutomationId="RoofSolar_TxtSaveDialogTitle" />
          <TextBox Text="{Binding SaveProjectName}"
                   Watermark="{loc:Translate ProjectName}"
                   Classes="Outlined"
                   AutomationProperties.AutomationId="RoofSolar_TxtBoxProjectName" />
          <Grid ColumnDefinitions="*,12,*">
            <Button Grid.Column="0"
                    Classes="Outlined"
                    Command="{Binding CancelSaveProjectCommand}"
                    HorizontalAlignment="Stretch"
                    HorizontalContentAlignment="Center"
                    Content="{loc:Translate Cancel}"
                    AutomationProperties.AutomationId="RoofSolar_BtnCancelSave" />
            <Button Grid.Column="2"
                    Classes="Primary"
                    Command="{Binding ConfirmSaveProjectCommand}"
                    HorizontalAlignment="Stretch"
                    HorizontalContentAlignment="Center"
                    Content="{loc:Translate Save}"
                    AutomationProperties.AutomationId="RoofSolar_BtnConfirmSave" />
          </Grid>
        </StackPanel>
      </Border>
    </Border>

  </Grid>

</UserControl>
